{
  "schemaVersion": 1,
  "id": "elevator",
  "version": "${version}",
  "name": "Elevator Mod",
  "description": "Adds animated elevator to Minecraft",
  "authors": [
    "CoalTrain"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "CC0-1.0",
  "icon": "assets/elevator/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "coaltrain.elevator.Elevator"
    ],
    "client": [
      "coaltrain.elevator.ElevatorClient"
    ]
  },
  "mixins": [
    "elevator.mixins.json",
    {
      "config": "elevator.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.17.3",
    "minecraft": "~1.21.8",
    "java": ">=21",
    "fabric-api": "*",
    "geckolib": ">=5.2.2"
  },
  "suggests": {
    "another-mod": "*"
  }
}